<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
  <title>#{title}</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/ant-design-vue@1.6.3/dist/antd.min.css">
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.runtime.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/ant-design-vue@1.6.3/dist/antd.min.js"></script>
  <script src="http://test--bucket.test.upcdn.net/lib.umd.min.js"></script>
</head>

<body>
  <div id="app"></div>

  script.
    window.__data = !{components}

  script.
    var __app = new Vue({
      render(h) {
        function renderChildren(component) {
          if (component.children) {
            return component.children.map(child => {
              return h(child.tag, child, renderChildren(child))
            })
          }
          return []
        }

        var content = window.__data.map(function (component) {
          return h(component.tag, component, renderChildren(component))
        })

        return h('div', {}, content)
      }
    }).$mount('#app')
</body>

</html>